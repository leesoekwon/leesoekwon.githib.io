datasource db {
  provider = "postgresql" // 혹은 mysql, sqlite 등 선택
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id         Int     @id @default(autoincrement())
  title      String
  content    String
  createdAt  DateTime @default(now())
  views      Int     @default(0)
  isNotice   Boolean @default(false)
}
